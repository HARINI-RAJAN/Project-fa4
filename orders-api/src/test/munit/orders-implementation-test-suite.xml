		<munit:execution >
			<flow-ref doc:name="Flow-ref to PostOrderDetailsFlow" doc:id="1939a1ec-263a-4179-b629-05d5cb5445ee" name="PostOrderDetailsFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="bbbe5e83-ad89-4891-b7c2-b169e15467c7" is="#[MunitTools::notNullValue()]" message="Payload is empty" expression="#[payload]"/>
		</munit:validation>
	</munit:test>


</mule>
