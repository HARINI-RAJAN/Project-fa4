	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="orders-implementation-test-suite.xml" />
	<munit:test name="orders-implementation-test-suite-GetAllOrderDetailsFlowTest" doc:id="e4ede44d-e01b-418f-b97d-a15f741446b6" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to GetAllOrderDetailsFlow" doc:id="2d0377a5-2c27-4dff-87f1-697629323a72" name="GetAllOrderDetailsFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="0bff3382-beab-4b36-9edf-7db33a340358" message="Payload is empty" expression="#[payload.orderNumber]" is="#[MunitTools::notNullValue()]"/>
		</munit:validation>
	</munit:test>
	<munit:test name="orders-implementation-test-suite-getOrderDetailsByIDFlowTest" doc:id="4e43872f-c876-49fb-a6cd-bf5523f3c6e0" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to getOrderDetailsByIDFlow" doc:id="9fd9b19e-da83-4fa5-b556-17f1589c2bf1" name="getOrderDetailsByIDFlow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="fbb2e12b-97e2-47c8-b414-b54db362b3a1" message="not found" expression="#[payload.orderNumber]" is="#[MunitTools::notNullValue()]"/>
		</munit:validation>
	</munit:test>
